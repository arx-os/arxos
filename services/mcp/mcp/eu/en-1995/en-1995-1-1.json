{
  "mcp_id": "en-1995-1-1",
  "name": "Eurocode 5: Design of Timber Structures - Part 1-1: General Rules and Rules for Buildings",
  "version": "2021",
  "jurisdiction": {
    "country": "EU",
    "region": "European Union",
    "authority": "CEN",
    "effective_date": "2021-01-01"
  },
  "description": "Eurocode 5 Part 1-1 provides general rules for the design of timber structures including materials, structural analysis, and design of structural members.",
  "rules": [
    {
      "rule_id": "EN1995-1-1-3.1.1",
      "name": "Timber Material Properties",
      "description": "Structural timber shall have specified strength classes",
      "category": "structural_materials",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "strength_class",
          "operator": "in",
          "value": ["C14", "C16", "C18", "C20", "C22", "C24", "C27", "C30", "C35", "C40", "C45", "C50", "D18", "D20", "D22", "D24", "D27", "D30", "D35", "D40", "D50", "D60", "D70"]
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber strength class must be specified according to EN 1995-1-1-3.1.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-3.1.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-3.2.1",
      "name": "Timber Service Classes",
      "description": "Timber shall be classified according to service conditions",
      "category": "structural_materials",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "service_class",
          "operator": "in",
          "value": [1, 2, 3]
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber service class must be specified per EN 1995-1-1-3.2.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-3.2.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-4.1.1",
      "name": "Structural Analysis - Linear Elastic",
      "description": "Linear elastic analysis may be used for ULS and SLS",
      "category": "structural_analysis",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "structural_element",
          "property": "analysis_method",
          "operator": "in",
          "value": ["linear_elastic", "non_linear", "plastic"]
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Structural analysis method must be specified per EN 1995-1-1-4.1.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-4.1.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-5.1.1",
      "name": "Ultimate Limit States - Tension",
      "description": "Timber members shall be verified for tension resistance",
      "category": "structural_safety",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "tension_verification",
          "operator": "==",
          "value": true
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber elements must be verified for tension per EN 1995-1-1-5.1.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-5.1.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-5.2.1",
      "name": "Ultimate Limit States - Compression",
      "description": "Timber members shall be verified for compression resistance",
      "category": "structural_safety",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "compression_verification",
          "operator": "==",
          "value": true
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber elements must be verified for compression per EN 1995-1-1-5.2.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-5.2.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-5.3.1",
      "name": "Ultimate Limit States - Bending",
      "description": "Timber members shall be verified for bending resistance",
      "category": "structural_safety",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "bending_verification",
          "operator": "==",
          "value": true
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber elements must be verified for bending per EN 1995-1-1-5.3.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-5.3.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-5.4.1",
      "name": "Ultimate Limit States - Shear",
      "description": "Timber members shall be verified for shear resistance",
      "category": "structural_safety",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "shear_verification",
          "operator": "==",
          "value": true
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber elements must be verified for shear per EN 1995-1-1-5.4.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-5.4.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-6.1.1",
      "name": "Serviceability Limit States - Deflection",
      "description": "Deflection limits shall be checked for timber structures",
      "category": "serviceability",
      "severity": "warning",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "deflection_checked",
          "operator": "==",
          "value": true
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Deflection limits should be checked for timber structures per EN 1995-1-1-6.1.1",
          "severity": "warning",
          "code_reference": "EN 1995-1-1-6.1.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-7.1.1",
      "name": "Detailing - Minimum Dimensions",
      "description": "Timber members shall have minimum dimensions for stability",
      "category": "structural_materials",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "minimum_width",
          "operator": ">=",
          "value": 38
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber elements must have minimum width of 38mm per EN 1995-1-1-7.1.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-7.1.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-8.1.1",
      "name": "Detailing - Connections",
      "description": "Timber connections shall be designed according to standards",
      "category": "structural_materials",
      "severity": "error",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_connection",
          "property": "connection_type",
          "operator": "in",
          "value": ["nailed", "screwed", "bolted", "doweled", "glued"]
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber connections must be properly designed per EN 1995-1-1-8.1.1",
          "severity": "error",
          "code_reference": "EN 1995-1-1-8.1.1"
        }
      ]
    },
    {
      "rule_id": "EN1995-1-1-9.1.1",
      "name": "Detailing - Moisture Protection",
      "description": "Timber shall be protected from excessive moisture",
      "category": "durability",
      "severity": "warning",
      "enabled": true,
      "conditions": [
        {
          "type": "property",
          "element_type": "timber_element",
          "property": "moisture_protection",
          "operator": "==",
          "value": true
        }
      ],
      "actions": [
        {
          "type": "validation",
          "message": "Timber should be protected from excessive moisture per EN 1995-1-1-9.1.1",
          "severity": "warning",
          "code_reference": "EN 1995-1-1-9.1.1"
        }
      ]
    }
  ]
} 