{
  "Basic Rectangle": {
    "prefix": "rect",
    "body": [
      "element rectangle(id=\"${1:rect1}\", x=${2:0}, y=${3:0}, width=${4:100}, height=${5:50})"
    ],
    "description": "Create a basic rectangle element"
  },
  "Basic Circle": {
    "prefix": "circle",
    "body": [
      "element circle(id=\"${1:circle1}\", x=${2:0}, y=${3:0}, radius=${4:25})"
    ],
    "description": "Create a basic circle element"
  },
  "Basic Line": {
    "prefix": "line",
    "body": [
      "element line(id=\"${1:line1}\", x1=${2:0}, y1=${3:0}, x2=${4:100}, y2=${5:0})"
    ],
    "description": "Create a basic line element"
  },
  "Structural Beam": {
    "prefix": "beam",
    "body": [
      "element beam(id=\"${1:beam1}\", x=${2:0}, y=${3:0}, length=${4:100}, material=\"steel\", elastic_modulus=${5:200e9})"
    ],
    "description": "Create a structural beam element"
  },
  "Structural Column": {
    "prefix": "column",
    "body": [
      "element column(id=\"${1:column1}\", x=${2:0}, y=${3:0}, height=${4:300}, material=\"concrete\", elastic_modulus=${5:30e9})"
    ],
    "description": "Create a structural column element"
  },
  "Pipe Element": {
    "prefix": "pipe",
    "body": [
      "element pipe(id=\"${1:pipe1}\", x=${2:0}, y=${3:0}, length=${4:100}, diameter=${5:50}, material=\"steel\")"
    ],
    "description": "Create a pipe element"
  },
  "Electrical Resistor": {
    "prefix": "resistor",
    "body": [
      "element resistor(id=\"${1:r1}\", x=${2:0}, y=${3:0}, resistance=${4:100})"
    ],
    "description": "Create an electrical resistor"
  },
  "Electrical Capacitor": {
    "prefix": "capacitor",
    "body": [
      "element capacitor(id=\"${1:c1}\", x=${2:0}, y=${3:0}, capacitance=${4:1e-6})"
    ],
    "description": "Create an electrical capacitor"
  },
  "Electrical Inductor": {
    "prefix": "inductor",
    "body": [
      "element inductor(id=\"${1:l1}\", x=${2:0}, y=${3:0}, inductance=${4:1e-3})"
    ],
    "description": "Create an electrical inductor"
  },
  "Voltage Source": {
    "prefix": "voltage",
    "body": [
      "element voltage_source(id=\"${1:v1}\", x=${2:0}, y=${3:0}, voltage=${4:12})"
    ],
    "description": "Create a voltage source"
  },
  "Current Source": {
    "prefix": "current",
    "body": [
      "element current_source(id=\"${1:i1}\", x=${2:0}, y=${3:0}, current=${4:1})"
    ],
    "description": "Create a current source"
  },
  "Structural Simulation": {
    "prefix": "sim-structural",
    "body": [
      "simulate structural {",
      "  elements: [${1:beam1}, ${2:column1}],",
      "  loads: [",
      "    {type: \"point_load\", magnitude: ${3:1000}, position: [${4:50}, ${5:0}]}",
      "  ],",
      "  constraints: [",
      "    {type: \"fixed\", element: \"${6:column1}\"}",
      "  ]",
      "}"
    ],
    "description": "Create a structural simulation"
  },
  "Electrical Simulation": {
    "prefix": "sim-electrical",
    "body": [
      "simulate electrical {",
      "  elements: [${1:r1}, ${2:c1}, ${3:v1}],",
      "  analysis: \"${4:dc}\",",
      "  frequency: ${5:60}",
      "}"
    ],
    "description": "Create an electrical simulation"
  },
  "Fluid Dynamics Simulation": {
    "prefix": "sim-fluid",
    "body": [
      "simulate fluid_dynamics {",
      "  elements: [${1:pipe1}],",
      "  fluid: \"${2:water}\",",
      "  inlet_velocity: ${3:1.0},",
      "  outlet_pressure: ${4:0}",
      "}"
    ],
    "description": "Create a fluid dynamics simulation"
  },
  "Heat Transfer Simulation": {
    "prefix": "sim-heat",
    "body": [
      "simulate heat_transfer {",
      "  elements: [${1:beam1}],",
      "  temperature_bc: {",
      "    inlet: ${2:373}, // 100°C",
      "    outlet: ${3:293}  // 20°C",
      "  }",
      "}"
    ],
    "description": "Create a heat transfer simulation"
  },
  "RF Propagation Simulation": {
    "prefix": "sim-rf",
    "body": [
      "simulate rf_propagation {",
      "  environment: \"${1:urban}\",",
      "  frequency: ${2:2.4e9}, // 2.4 GHz",
      "  transmitters: [",
      "    {id: \"${3:tx1}\", position: [${4:0}, ${5:0}, ${6:10}], power: ${7:20}}",
      "  ]",
      "}"
    ],
    "description": "Create an RF propagation simulation"
  },
  "Comment Block": {
    "prefix": "comment",
    "body": [
      "/*",
      " * ${1:Comment description}",
      " */"
    ],
    "description": "Create a comment block"
  },
  "Region": {
    "prefix": "region",
    "body": [
      "// #region ${1:Region name}",
      "$2",
      "// #endregion"
    ],
    "description": "Create a code region"
  }
} 