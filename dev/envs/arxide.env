# ArxIDE Environment Configuration

# Service Configuration
ARXIDE_NAME=ArxIDE Desktop
ARXIDE_VERSION=1.0.0
ARXIDE_ENVIRONMENT=development
ARXIDE_LOG_LEVEL=INFO

# Tauri Configuration
ARXIDE_TAURI_HOST=0.0.0.0
ARXIDE_TAURI_PORT=8080
ARXIDE_TAURI_DEV_SERVER=http://localhost:3000

# Integration Configuration
ARXIDE_GUS_API_URL=http://gus:9001
ARXIDE_BACKEND_API_URL=http://backend:4000
ARXIDE_SVGX_ENGINE_URL=http://localhost:8000

# Canvas 2D Configuration
ARXIDE_CANVAS_WIDTH=1920
ARXIDE_CANVAS_HEIGHT=1080
ARXIDE_GRID_SIZE=20
ARXIDE_SNAP_ENABLED=true
ARXIDE_PRECISION_LEVEL=0.1mm

# Native System Access
ARXIDE_FILE_SYSTEM_ENABLED=true
ARXIDE_CLI_INTEGRATION=true
ARXIDE_ARX_WALLET_ENABLED=true

# Performance Configuration
ARXIDE_MAX_OBJECTS=50000
ARXIDE_RENDER_FPS=60
ARXIDE_MEMORY_LIMIT=2048MB
ARXIDE_CACHE_SIZE=500MB

# Development Configuration
ARXIDE_DEBUG_MODE=true
ARXIDE_HOT_RELOAD=true
ARXIDE_DEV_MODE=true 