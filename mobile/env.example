# ArxOS Mobile Environment Configuration

# API Configuration
API_BASE_URL=http://localhost:8080/api/v1
API_TIMEOUT=30000

# Development Settings
DEBUG_MODE=true
LOG_LEVEL=debug

# Sync Configuration
AUTO_SYNC_INTERVAL=300000
MAX_RETRIES=3
